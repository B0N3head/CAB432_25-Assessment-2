JWT_SECRET=
PORT=3000
AWS_REGION=ap-southeast-2

# Cognito (set all to enable Cognito auth + groups + MFA) - Comment out for local dev
COGNITO_USER_POOL_ID=
COGNITO_CLIENT_ID=
COGNITO_DOMAIN=
# Must match exactly what's registered in Cognito callback URLs
COGNITO_REDIRECT_URI=

# S3 Configuration
S3_BUCKET=
S3_UPLOADS_PREFIX=media/uploads/
S3_OUTPUTS_PREFIX=media/outputs/
S3_THUMBS_PREFIX=media/thumbnails/

# ElastiCache Memcached (optional caching and SSE progress store)
MEMCACHED_URL=

# Optional: Parameter Store prefix for centralized config
SSM_PARAM_PREFIX=/app/video-editor

