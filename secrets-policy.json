{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "VideoEditorSecretsAccess",
            "Effect": "Allow",
            "Action": [
                "secretsmanager:GetSecretValue",
                "secretsmanager:DescribeSecret"
            ],
            "Resource": [
                "arn:aws:secretsmanager:ap-southeast-2:*:secret:video-editor/*"
            ]
        }
    ]
}